<text_conversions version="1.0">
    <!-- ALL P-TAGS -->
    <conversion_rule id="strip_all_p_tags">
        <conversion cms_char="" np_char="&lt;p>"/>
        <conversion cms_char="" np_char="&lt;/p>"/>
        <conversion cms_char="" np_char="&lt;p/>"/>
    </conversion_rule>
    <!-- REPLACE P-TAG WITH COMMA -->
    <conversion_rule id="replace_p_tag_with_comma">
        <conversion cms_char=", " np_char="&lt;/p>&lt;p>"/>
    </conversion_rule>
    <!-- ALL B-TAGS -->
